            <link href="/Content/plugins/mapplic/css/mapplic-ie.css" rel="stylesheet" type="text/css" />
    <![endif]-->
    <script type="text/javascript">
        window.onload = function () {
            // fix for windows 8
            if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
                document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="/Content/css/windows.chrome.fix.css" />'
        }
    </script>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



</head>
<body class="fixed-header  dashboard ">
    @using Garia.Core.Handlers
    @using Garia.Core.Entities


    <!-- BEGIN SIDEBPANEL-->
    <nav class="page-sidebar" data-pages="sidebar">
        <!-- BEGIN SIDEBAR MENU HEADER-->
        <div class="sidebar-header">
            <img src="~/Content/img/logo_white_2x.png" width="78" height="22" />
            <div class="sidebar-header-controls">
                <!--<button type="button" class="btn btn-xs sidebar-slide-toggle btn-link m-l-20" data-pages-toggle="#appMenu"><i class="fa fa-angle-down fs-16"></button></i>-->
                <!--<button type="button" class="btn btn-link visible-lg-inline" data-toggle-pin="sidebar"><i class="fa fs-12"></i></button>-->
            </div>
        </div>
        <!-- END SIDEBAR MENU HEADER-->
        <!-- START SIDEBAR MENU -->
        <div class="sidebar-menu">
            <!-- BEGIN SIDEBAR MENU ITEMS-->
            @{/*
                string dashboardOpen = (ViewBag.PageID == "DASHBOARD") ? "open" : "";
                string dashboardBG = (ViewBag.PageID == "DASHBOARD") ? "bg-success" : "";
                string socialOpen = (ViewBag.PageID == "SOCIAL") ? "open" : "";
                string socialBG = (ViewBag.PageID == "SOCIAL") ? "bg-success" : "";
                string reportOpen = (ViewBag.PageID == "REPORT") ? "open" : "";
                string reportBG = (ViewBag.PageID == "REPORT") ? "bg-success" : "";
               */
            }
            <br />
            <ul class="menu-items">
                <li>
                    <a href="/">
                        <span class="title">Dashboard</span>
                        <span class="icon-thumbnail "><i class="fa fa-dashboard"></i></span>
                    </a>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title">Messages</span>
                        <span class="icon-thumbnail" style="font-size:12px">

                            @MessageHandler.GetunreadById(Convert.ToInt32(User.Identity.Name))

                            <i class="fa fa-envelope"></i>
                        </span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("Messages", "Message")">
                                <span class="title">Messages</span>
                                <span class="icon-thumbnail"><i class="fa fa-envelope"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("SentMessages", "Message")">
                                Sent Messages
                                <span class="icon-thumbnail"><i class="fa fa-envelope-o"></i></span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title">Tasks</span>
                        <span class="icon-thumbnail" style="font-size:12px">
                            @TaskHandler.GetUnconfirmedTaskNumber(Convert.ToInt32(User.Identity.Name))
                            <i class="fa fa-tasks"></i>
                        </span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("Index", "Task")">
                                <span class="title">My Tasks</span>
                                <span class="icon-thumbnail">
                                    <i class="fa fa-check-square-o"></i>
                                </span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CreateTask", "Task")">
                                <span class="title">New Task</span>
                                <span class="icon-thumbnail">
                                    <i class="fa fa-plus"></i>
                                </span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CreatedTask", "Task")">
                                Created Tasks
                                <span class="icon-thumbnail"><i class="fa fa-check"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CompletedTask", "Task")">
                                Completed Tasks
                                <span class="icon-thumbnail"><i class="fa fa-check"></i></span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="@Url.Action("Social", "Social")">
                        <span class="title">Social</span>
                        <span class="icon-thumbnail"><i class="fa fa-users"></i></span>
                    </a>
                </li>
                <li class="">
                    <a href="@Url.Action("HighScore", "HighScore")">
                        <span class="title">HighScore</span>
                        <span class="icon-thumbnail"><i class="fa fa-trophy"></i></span>
                    </a>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title arrow">Budget / Sales</span>
                        <span class="icon-thumbnail"><i class="fa fa-line-chart"></i></span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("BudgetSales", "Graph")">
                                Sales Managers
                                <span class="icon-thumbnail"><i class="fa fa-line-chart"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("BudgetSalesDealer", "Graph")">
                                Dealers
                                <span class="icon-thumbnail"><i class="fa fa-line-chart"></i></span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title arrow">Sales</span>
                        <span class="icon-thumbnail"><i class="fa fa-usd"></i></span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("SaleLog", "Sale")">
                                Sale Log
                                <span class="icon-thumbnail"><i class="fa fa-money"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CreateSale", "Sale")">
                                Create Sale
                                <span class="icon-thumbnail"><i class="fa fa-usd"></i></span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title arrow">Dealers</span>
                        <span class="icon-thumbnail"><i class="fa fa-plus"></i></span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("Dealers", "Dealer")">
                                Dealers
                                <span class="icon-thumbnail"><i class="fa fa-plus"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CreateDealer", "Dealer")">
                                Create New Dealer
                                <span class="icon-thumbnail"><i class="fa fa-plus"></i></span>
                            </a>
                        </li>
                        <li class="">
                            <a href="@Url.Action("CreateDealerRelationship", "Dealer")">
                                Create Relationship
                                <span class="icon-thumbnail">sp</span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="javascript:;">
                        <span class="title arrow">Cars</span>
                        <span class="icon-thumbnail"><i class="fa fa-car"></i></span>
                    </a>
                    <ul class="sub-menu">
                        <li class="">
                            <a href="@Url.Action("Cars", "Car")">
                                Cars
                                <span class="icon-thumbnail"><i class="fa fa-car"></i></span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="">
                    <a href="@Url.Action("Employee", "Employee")" class="elements">
                        <span class="title">Employee</span>
                        <span class="icon-thumbnail "><i class="fa fa-user"></i></span>
                    </a>
                </li>

                <li class="">
                    <a href="@Url.Action("LogOff", "Account")" class="elements">
                        <span class="title">Log Out</span>
                        <span class="icon-thumbnail "><i class="fa fa-sign-out"></i></span>
                    </a>
                </li>

            </ul>
        </div>

        <!-- END SIDEBAR MENU -->
    </nav>
    <!-- END SIDEBAR -->
    <!-- END SIDEBPANEL-->
    <!-- START PAGE-CONTAINER -->
    <div class="page-container">
        <!-- START HEADER -->
        <div class="header ">
            <!-- START MOBILE CONTROLS -->
            <!-- LEFT SIDE -->
            <div class="pull-left full-height visible-sm visible-xs">
                <!-- START ACTION BAR -->
                <div class="sm-action-bar">
                    <a href="#" class="btn btn-link toggle-sidebar" data-toggle="sidebar">
                        <span class="icon-set menu-hambuger"></span>
                    </a>
                </div>
                <!-- END ACTION BAR -->
            </div>
            <!-- END MOBILE CONTROLS -->
            <div class="pull-left sm-table">
                <div class="header-inner">
                    <div class="brand inline">
                        <img src="~/Content/img/logo_2x.png" width="78" height="22">

                    </div>

                </div>
            </div>
            <div class="pull-right">
                <div class="header-inner">
                    
                </div>
            </div>
            <div class="pull-right">
                <!-- START User Info-->
                <div class="visible-lg visible-md m-t-10">

                    @{
                        Employee Employee = EmployeeHandler.GetEmployeeById(Convert.ToInt32(User.Identity.Name));

                    if(Employee != null){
                        <div class="pull-left p-r-10 p-t-10 fs-16 font-heading"><span class="text-master">@Employee.Fname @Employee.Lname </span></div>
                    }
                    
                        <div class="thumbnail-wrapper d32 circular inline m-t-5">
                            <img src="/content/img/profiles/avatar.png" alt="" data-src="/content/img/profiles/avatar.png" data-src-retina="/content/img/profiles/avatar.png" width="32" height="32">
                        </div>
                    }

                </div>
                <!-- END User Info-->
            </div>
        </div>
        <div class="page-content-wrapper">
            <!-- START PAGE CONTENT -->
            <div class="content sm-gutter">

                <div class="container-fluid padding-25 sm-padding-10">
                    @RenderBody()
                </div>
            </div>
        </div>


        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)

        <!-- BEGIN VENDOR JS -->
        <script src="/Content/plugins/pace/pace.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery/jquery-1.8.3.min.js" type="text/javascript"></script>


        <script src="/Content/plugins/modernizr.custom.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
