        <script src="~/Content/plugins/jquery-validation/js/jquery.validate.min.js" type="text/javascript"></script>
        <script src="~/Content/plugins/jquery-validation/js/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-bez/jquery.bez.min.js"></script>
        <script src="/Content/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-actual/jquery.actual.min.js"></script>
        <script src="/Content/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
        <script src="/Content/plugins/bootstrap-select2/select2.min.js"></script>
        <script src="/Content/plugins/classie/classie.js"></script>
        <script src="/Content/plugins/switchery/js/switchery.min.js" type="text/javascript"></script>

        <script src="/Content/plugins/nvd3/test/d3.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/nvd3/test/nv.d3.min.js" type="text/javascript"></script>

        <script src="/Content/plugins/mapplic/js/hammer.js"></script>
        <script src="/Content/plugins/mapplic/js/jquery.mousewheel.js"></script>
        <script src="/Content/plugins/mapplic/js/mapplic.js"></script>
        <script src="/Content/plugins/rickshaw/rickshaw.min.js"></script>
        <script src="/Content/plugins/jquery-metrojs/MetroJs.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-sparkline/jquery.sparkline.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/skycons/skycons.js" type="text/javascript"></script>
        <script src="/Content/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-numberAnimate/jquery.animateNumbers.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-isotope/isotope.pkgd.min.js" type="text/javascript"></script>
        <script src="/Content/plugins/jquery-isotope/masonry-horizontal.js" type="text/javascript"></script>
        <script src="/Content/js/core.js" type="text/javascript"></script>
        <script src="/Content/js/default.js" type="text/javascript"></script>

        <!-- END VENDOR JS -->
        <!-- BEGIN CORE TEMPLATE JS -->
        <script src="/Content/js/pages.min.js"></script>
        <!-- END CORE TEMPLATE JS -->
        <!-- BEGIN PAGE LEVEL JS -->
        <script src="/Content/js/myDashboard.js" type="text/javascript"></script>
        <script src="/Content/js/MyChartJS.js" type="text/javascript"></script>

        <script src="/Content/js/scripts.js" type="text/javascript"></script>
        <script src="/Content/js/pages.social.js" type="text/javascript"></script>
        <!-- END PAGE LEVEL JS -->
        <!--NOTE: When using ajax begin form always load this script after other or else it wont work-->
        <script src="~/Scripts/jquery.unobtrusive-ajax.js" type="text/javascript"></script>

</body>
</html>
